{
  "root": true,
  "globals": {
    "document": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "func-names": 0,
    "no-plusplus": 0,
    "arrow-parens": 0,
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ]
}
